.double-bounce1,.double-bounce2 {
	width: 100%;
	height: 100%;
	border-radius: 50%;
	background-color: #c90000;
	opacity: .6;
	position: absolute;
	top: 0;
	left: 0;
	-webkit-animation: bounce 2s infinite ease-in-out;
	animation: bounce 2s infinite ease-in-out
}

.double-bounce2 {
	-webkit-animation-delay: -1s;
	animation-delay: -1s
}

@-webkit-keyframes bounce {
	0%,100% {
		-webkit-transform: scale(0)
	}

	50% {
		-webkit-transform: scale(1)
	}
}

@keyframes bounce {
	0%,100% {
		transform: scale(0);
		-webkit-transform: scale(0)
	}

	50% {
		transform: scale(1);
		-webkit-transform: scale(1)
	}
}

.scale-transition {
	opacity: 1;
	transform: scale(1);
	-webkit-transform: scale(1);
	transition: all .3s ease-in-out;
	transition: all .3s ease-in-out
}

.scale-enter {
	transform: scale(0);
	-webkit-transform: scale(0);
	opacity: 0
}

.scale-leave {
	display: none
}

.fadeIn-transition {
	opacity: 1;
	transition: all .3s ease-in-out;
	transition: all .3s ease-in-out
}

.fadeIn-enter {
	opacity: 0
}

.fadeIn-leave {
	display: none
}

.silderDown-transition {
	height: auto;
	transition: all 1.3s ease-in-out;
	transition: all 1.3s ease-in-out
}

.silderDown-enter {
	height: 0
}

.silderDown-leave {
	display: none
}

.loader-inner {
	width: 50px;
	height: 50px;
