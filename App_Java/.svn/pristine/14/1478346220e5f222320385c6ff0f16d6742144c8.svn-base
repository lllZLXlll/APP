<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.pjzb.dao.ExGoInDao">

	<select id="queryExGoMony" resultType="map">
		SELECT * FROM t_exGoIn
		WHERE userId = 1
	</select>

	<update id="updateExGoSum">
		UPDATE t_exGoIn SET invmAmount = #{1}
		WHERE userId = 1
	</update>

</mapper>





