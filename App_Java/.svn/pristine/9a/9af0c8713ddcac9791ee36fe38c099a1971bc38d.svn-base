.section .fadeT{opacity:0;-webkit-transition: all 1s;transition: all 1s;-ms-transform:translateY(-200px);-webkit-transform:translateY(-300px);transform:translateY(-300px);}
.section.active .fadeT{opacity:1;-webkit-transition-delay:0.3s;transition-delay:0.3s;-ms-transform:translateY(0);-webkit-transform:translateY(0);transform:translateY(0);}

.section .fadeB{opacity:0;-webkit-transition: all 1s;transition: all 1s;-ms-transform:translateY(200px);-webkit-transform:translateY(300px);transform:translateY(300px);}
.section.active .fadeB{opacity:1;-webkit-transition-delay:0.3s;transition-delay:0.3s;-ms-transform:translateY(0);-webkit-transform:translateY(0);transform:translateY(0);}

.section .fadeL{opacity:0;-webkit-transition: all 1s;transition: all 1s;-ms-transform:translateX(-200px);-webkit-transform:translateX(-300px);transform:translateX(-300px);}
.section.active .fadeL{opacity:1;-webkit-transition-delay:0.3s;transition-delay:0.3s;-ms-transform:translateX(0);-webkit-transform:translateX(0);transform:translateX(0);}

.section .fadeR{opacity:0;-webkit-transition: all 1s;transition: all 1s;-ms-transform:translateX(200px);-webkit-transform:translateX(300px);transform:translateX(300px);}
.section.active .fadeR{opacity:1;-webkit-transition-delay:0.3s;transition-delay:0.3s;-ms-transform:translateX(0);-webkit-transform:translateX(0);transform:translateX(0);}

.section .rollinL{opacity:0;-webkit-transition: all 1s;transition: all 1s;-webkit-transform:translateX(-200%) rotate(-240deg);-ms-transform:translateX(-200%) rotate(-240deg);transform:translateX(-200%) rotate(-240deg);-webkit-transition-delay:0.3s;transition-delay:0.3s;}
.section.active .rollinL{opacity:1;-webkit-transform:translateX(0px) rotate(0deg);-ms-transform:translateX(0px) rotate(0deg);transform:translateX(0px) rotate(0deg);-webkit-transition-delay:0.3s;transition-delay:0.3s;}



