/* reset */
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video,input{margin: 0;padding: 0;border: 0;outline: 0;font-size:100%;vertical-align: baseline;background: transparent;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}body{font-family: 'Helvetica Neue', 'Roboto', "microsoft yahei";line-height: 1;-webkit-touch-callout: none; -webkit-text-size-adjust: none;}
hr{padding:0;margin:0;}
html,body{height:100%;background:#fff}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block;}
ul{list-style:none;}
blockquote,q{quotes:none;}
blockquote:before,blockquote:after,q:before,q:after{content:''; content:none;}
a{margin:0;padding:0;color:#1e1e1e;font-size:100%;vertical-align:baseline;background:transparent;text-decoration:none;}
a:hover{color:#00afeb;}
em,i{font-style:normal;}
h1,h2,h3,h4,h5{}

/*iBanner*/
.banner{position: relative;}
.iBanner{height: 100%;position: relative;float: left;width: 100%;}
.iBanner .iBanner-box{position: relative;overflow: hidden;height: 100%;}
.iBanner .iBanner-box .box{display: none;height: 100%;background-position: center center;background-repeat: no-repeat;}
.iBanner .iBanner-box .box-active{display: block;}
.iBanner .iBanner-box .box a{display: block;height: 100%;position: relative;}
.iBanner .iBanner-box .box-bg{position: absolute;left: 0;top: 0;z-index: 0;width: 100%;height: 100%;}

/*iBanner custom*/
.iBanner .iBanner-box .fd-btn{position: absolute;left: 50%;top: 50%;margin: 10px 0 0 176px;}
.iBanner .iBanner-box .fd-btn .icon{width: 292px;height: 82px;display: block;}
.iBanner .iBanner-box .wdj-btn{position: absolute;left: 50%;bottom: 20px;margin: 0 0 0 -90px;}
.iBanner .iBanner-box .wdj-btn .icon-wdj{width: 180px;height: 40px;display: block;}
.iBanner .iBanner-box .azb-btn{position: absolute; left: 50%; bottom: 36px; margin: 0 0 0 338px;}
.iBanner .iBanner-box .azb-btn .icon-azb{width: 124px; height: 28px; display: block;}
.iBanner .previous,
.iBanner .next{position: absolute; height: 116px; width: 62px; margin-top: -20px; z-index: 2000; top: 50%; left: 50%; margin-left: -32px; display: none;}
.iBanner .previous{background-position: -3px -30px;}
.iBanner .next{background-position: -56px -30px;}
.prev-mask{position: absolute; left: 0; width: 180px; height: 100%; z-index: 1000; top: 0; display: none;}
.next-mask{position: absolute; right: 0; width: 180px; height: 100%; z-index: 10; top: 0; display: none;}
.prev-mask,
.next-mask{background-repeat: no-repeat; background-position: center;}
.iBanner-prev .prev-mask .previous,
.iBanner-next .next-mask .next{display: block; cursor: pointer;}
.iBanner .iBanner-box .fd-btn{position: absolute; left: 50%; top: 50%; margin: 68px 0 0 252px; z-index: 30;}
.iBanner .fd-btn .icon{display: block; width: 216px; height: 62px; text-indent: -99999px;}
.iBanner .fd-btn .icon img{display: block; margin: 0 auto;}
.iBanner .fd-btn .sub-text{margin-top: 6px;}
.iBanner .fd-btn .sub-text p{color: #fff; line-height: 24px;}
.iBanner-nav{height:30px; position:absolute; bottom:0; left:50%; _top:500px; z-index:2000;}
.iBanner-nav ul{overflow: hidden;}
.iBanner-nav ul li{float:left; margin:0 4px;}
.iBanner-nav ul li a{opacity:0.5; filter:Alpha(Opacity=50); display:block; width:10px; height:10px;}
.iBanner-nav ul li a img{display:block; width:100%; overflow:hidden;}
.iBanner-nav ul li a.active{opacity:1; filter:Alpha(Opacity=100);}

.warp-1000{position: relative;width: 1000px;margin: 0 auto;}
.warp-1000:after{
	content: "";display: block; height: 0; clear: both; visibility: hidden;
}
*+html .warp-1000{z-index:1;}/*IE7 hack*/
