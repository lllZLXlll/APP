.mod-index{
	background: #e8e8e8;
	padding-top: 36px;
	padding-bottom: 55px;
	.mod-data-list{
		margin-top: 30px;
	}
	.head{
		position: relative;
		height: 54px;
		line-height: 54px;
		h2{
			float: left;
			font-size: 18px;
			color: #1e1e1e;
			padding-left: 6px;
		}
		span{
			display: inline-block;
			border-radius: 3px;
			background: #00afeb;
			color: #fff;
			height: 12px;
			line-height: 12px;
			font-size: 12px;
			padding: 4px 8px;
			margin-left: 4px;
		}
		.more{
			position: absolute;
			right: 8px;
			color: #1e1e1e;
			font-size: 14px;
		}
	}
}
.mod-index-great{
	height: 62px;
	padding: 30px 0;
	background: #fff;
	border-radius: 3px;
	ul{
		width: 100%;
		height: 100%;
		li{
			display: block;
			float: left;
			width: 25%;
			.side{
				float: left;
				margin-left: 32px;
			}
			.main{
				padding-left: 104px;
				color: #1e1e1e;
				font-size: 14px;
				.title{
					color: #3180bf;
					font-size: 24px;
					padding-bottom: 12px;font-weight: bold;
				}
			}
		}
	} 
}
.mod-index-transform{
	margin-top: 30px;
	.table{
		padding: 0 16px 10px;
		background: #fff;
		border-radius: 3px;
	}
	table{
		background: #fff;
		width: 100%;
		border-collapse:collapse;
		border-spacing:0;
		table-layout:fixed;
		font-size: 14px;
		tr{
			td{
				position: relative;
				vertical-align: middle;
				padding: 14px 0;
				border-bottom: 1px solid #cbcbcb;
				width: 20%;
				text-align: center;
				overflow: hidden;
				&.red{
					color: #eb4600;
				}
				&.first{
					text-align: left;
					.box{
						overflow: hidden;
					}
				}
				&.last{
					text-align: right;
				}
			}
			&.last{
				td{
					border: none;
				}
			}
		}
		h3{
			font-size: 16px;
			color: #1e1e1e;
			.supply{
				background: #d58200;
				color: #fff;display: inline-block;
				padding: 2px 8px;
				margin-left: 4px;
				font-size: 12px;
			}
			.new{
				background: #5fad2b;
				color: #fff;display: inline-block;
				padding: 2px 8px;
				margin-left: 4px;
				font-size: 12px;
			}
		}
		.common-data-type{
			margin-top: 12px;
		}
		.btn{
			position: relative;
			top: 1px;
			display: inline-block;
			width: 110px;
			height: 35px;
			line-height: 34px;
			background: #3180bf;
			color: #FFF;
			font-size: 16px;
			text-align: center;
			border-radius: 3px;
		}
	}
}
.mod-index-new{
	overflow: hidden;
	padding-top: 34px;
	h2{
		font-weight: bold;
		font-size: 18px;
		color: #1e1e1e;
		padding: 24px 0;
	}
	ul{
		margin-top: -12px;
		li{
			font-size: 12px;
			margin-top: 12px;
			a{
				color: #1e1e1e;
			}
		}
	}
	.box{
		width: 50%;
		float: left;
		overflow: hidden;
	}
}
.mod-index-partner{
	padding: 30px 0 60px;
	h2{
		font-weight: bold;
		font-size: 18px;
		color: #1e1e1e;
		padding: 24px 0;
	}
	ul{font-size: 0;margin-left: -10px;}
	li{
		border:1px solid #d6d6d6;
		display: inline-block;margin-left: 10px;
		img{
			width: 114px;
			height: 50px;
		}
	}
}