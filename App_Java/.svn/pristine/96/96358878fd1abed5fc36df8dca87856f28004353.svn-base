.text-logo img{display:block;margin:0 auto;}
.text-h-t{font-size:16px;color:#3e3a39;line-height:32px;text-indent:32px;}

.text-banner{margin-top:40px;}
.credit-pic{position:relative;float:left;margin:0 5px 5px 0;cursor:pointer;}
.credit-pic>span{display:none;position:absolute;top:0;left:0;width:240px;height:200px;background-color:rgba(0,0,0,0.5);}
.credit-pic>span>span{width:240px;color:#fff;display:block;line-height:200px;text-align:center;}
.text-banner-two .credit-pic>span{width:163px;height:106px;}
.text-banner-two .credit-pic>span>span{width:163px;line-height:106px;}
.about-chengtou{overflow:hidden;}
.about-chengtou li{
	float:left;
	width:228px;
	margin:0 15px 15px 0;	
	
}
.about-chengtou li span{
	display:block;
	width:228px;
	height:30px;
	line-height:30px;
	color:#707070;
	font-size:14px;
	text-align:center;
}


.credit-left .credit-pic>span{width:200px;height:275px;}
.credit-left .credit-pic>span>span{width:200px;line-height:275px;}

.credit-right .credit-pic>span{width:190px;height:132px;}
.credit-right .credit-pic>span>span{width:190px;line-height:132px;}

.text-pic-photo-man{float:left;margin:0!important}
.text-pic-photo{float:left;padding-left:20px;padding-top: 30px;}
.text-pic-photo p{margin-bottom:10px!important;}
.text-pic-photo-name{font-size:18px!important;font-weight:bold;line-height:30px;border-bottom:1px solid #bcbcbc;}
.text-pic-photo-name span{font-size:16px!important;font-weight:500;display:inline-block;padding-left:10px;}
.text-pic-photo-zhiwei{font-size:16px;padding-bottom:20px;color:#3e3a39}
.text-pic-photo-jianjie{text-indent:32px;font-size:16px!important;line-height:32px;color:#3e3a39}
.about-adviser .text-pic-photo{height:auto;}
.text-pic-photo-name.small{font-size:16px!important;color:#3e3a39}


.job-mail{width:740px;height:50px;background-color:#e6002d;}
.job-mail strong{height:50px;line-height:50px;font-size:18px;color:#fff;float:left;padding-left:10px;}
.job-mail p{float:right;padding-right:10px;margin-bottom:0px!important;line-height:50px!important;font-size:16px!important;color:#fff;}
.job-mail p a{color:#ff9f00;}
.text-hover .job-top{height:48px;background-color:#e5e5e5;width:100%;}
.text-hover .job-top span{padding:0 20px;height:48px;line-height:48px;}
.text-hover .job-list{width:740px;}
.text-hover .job-list li a{display:block;width:685px;height:53px;padding:0 10px 0 45px;line-height:53px;background:url(shImages/job_more.png) no-repeat 25px center}
.text-hover .job-list li a.on{background:url(shImages/job_les.png) no-repeat 25px center}
.job-child{padding:10px 50px;display:none;}
.job-child .job-child-title{font-size:18px!important;color:#333;}


/*product*/
.pro-finan{padding:27px;background-color:#fff;}
.pro-name{border-bottom:1px solid #bcbcbc;padding-bottom:18px;}
.pro-name .border-r{border-right:1px solid #bdbdbd;padding:0 27px 0 10px;}
.pro-name>a{display:inline-block;padding:0 27px 0 27px;line-height:20px;font-size:18px;}
.pro-name>span{width:1px;height:20px;background-color:#bdbdbd;display:inline-block;}
.pro-name>a.on,.pro-name>a:hover{color:#e8403e;}
.pro-main{width:946px;}
.pro-main-top{width:944px;border:1px solid #595656;height:140px;margin-top:36px;}
.pro-main-top-one{width:230px;float:left;height:140px;background:url(shImages/pro_nianhua.png)no-repeat 10px top;background-color:#faf3f3;}

.pro-main-d .pro-main-top-one{background:url(shImages/pro_nianhua_01.png)no-repeat 10px top;}

.pro-main-top-one h3{padding:34px 0 34px 90px;color:#e93e56;font-size:24px;}
.pro-main-top-one p{height:30px;padding-left:52px;line-height:30px;background:url(shImages/pro_nianhua_bt.png)no-repeat 30px center;color:#434343;}
.pro-main-top-two{width:714px;float:left;height:107px;padding-top:33px;}
.pro-main-top-two p{font-size:18px;height:20px;line-height:20px;color:#434343;text-align:center;}
.pro-main-top-two h3{font-size:18px;text-align:center;line-height:31px;margin-top:12px;color:#434343;}
.pro-main-top-two h3 span{color:#e6002d;}
.pro-main-top-two>div{width:238px;float:left;}
.pro-main-center{padding-top:30px;}
.pro-main-center>div{width:473px;float:left;}
.pro-main-center>div.pro-main-js{padding-right:30px;width:443px;}
.pro-main-center>div>h3{line-height:30px;font-size:18px;}
.pro-main-center>div>p{line-height:30px;font-size:16px;color:#3f3b3a}

.pro-main-table>h3{height:55px;line-height:55px;}
.pro-main-table table{border:1px solid #8c8a8a;border-radius:10px;width:944px;}
.pro-main-table table td{padding:14px 0;text-align:center;vertical-align: middle;border-bottom:1px solid #8c8a8a;border-right:1px solid #8c8a8a;}
.pro-main-table table td.no-border{border-bottom:none;border-right:none;}
.pro-main-table table td.no-border-r{border-right:none;}
.pro-main-table table td.bg{background-color:#faf3f3}
.pro-main-table .have-bg{background:url(shImages/waring.png) no-repeat 218px center;}

.dialog_left,.dialog_right{width:50px;height:50px;cursor:pointer;background:url(shImages/banner_btn_l.png) no-repeat center center;position:absolute;left:-80px;top:300px;}
.dialog_right{left:1054px;background:url(shImages/banner_btn_r.png) no-repeat center center;}
