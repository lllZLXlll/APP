<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.pjzb.dao.FinanceDao">

	<insert id="addUserVoucher">
		insert into t_user_voucher(
			userId, rewardMoney, rewardTime, minMoney, maxMoney, rbname, passDate, realMoney
		)values(
			#{userId}, null, null, #{minMoney}, #{maxMoney},#{rbname}, #{passDate}, #{realMoney}
		)
	</insert>
	
</mapper>





