@charset "utf-8";
*{margin:0;padding:0;border:0;word-wrap: break-word}
ul,li {list-style-type: none;}

/*activity*/
.activity{
	width: 100%;
	height:100%;
	position: relative;
}
.activity .top{
	width: 100%;
	height:3.5rem;
	background:url(../images/activity_04/activity_top.png) top center /100% no-repeat ;
}
.activity_content{
	width:97%;
	padding: 1.2rem 0.1rem;
	float: left;
	background:#fff7dd url(../images/activity_04/activity_content.png)top center /100% no-repeat ;
}
.activity_content .content_center{
	width:100%;
	height:100%;
	position: relative;
}
.content_center ul{
	width:100%;
	float:left;
	margin:0.1rem 0.2rem 0 0.03rem;
}
.content_center ul li {
	float:left;
	width:2.8rem;
	height:3.25rem;
	margin:0.1rem 0.13rem;
}
.content_center ul li img{
	width:100%;
	height:100%;
}
.content_center .go-btn{
		display:block;
	width:1.86rem;
	height:0.55rem;
	line-height:0.55rem;
	font-size:0.30rem;
	border-radius:0.1rem;
	color:#fff;
	text-align:center;
	position:absolute;
	top:14.21rem;
	left:50%;
	margin-left:-0.85rem;
	background:#ff403a;
}
.footer{
	width:91%;
	padding: 0.3rem;
	float: left;
	background:#fff7dd;
}
.footer .title{
	margin:0 auto;
	width:100%;
	border-bottom: 1px solid #894e27;
	position:relative;
}
.footer .title .title_i{
	background:#fff7dd;
	width:1.9rem;
	height:0.42rem;
	position:absolute;
	left:50%;
	top:-0.16rem;
	margin-left:-0.9rem;
	font-size: 0.20rem;
    line-height: 0.42rem;
	color:#894e27;
	text-align: center;
}
.footer .text{
	margin:0.3rem auto;
	width:100%
}
.footer .text p{
	font-size: 0.18rem;
    line-height: 0.42rem;
	color:#894e27;
	text-indent: -0.28rem;
	padding-left: 0.28rem;
	
}