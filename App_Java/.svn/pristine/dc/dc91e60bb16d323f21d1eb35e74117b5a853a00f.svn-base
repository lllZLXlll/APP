@charset "utf-8";
* {
    word-wrap: break-word
}

html,
body,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
iframe,
dl,
dt,
dd,
ul,
ol,
li,
pre,
form,
button,
input,
textarea,
th,
td,
fieldset,
option {
    margin: 0;
    padding: 0;
    border: 0;
}

ul,
ol,
dl {
    list-style-type: none
}

html,
body {
    *position: static
}

html {
    font-family: sans-serif;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%
}

address,
caption,
cite,
code,
dfn,
em,
th,
var {
    font-style: normal;
    font-weight: normal
}

input,
button,
textarea,
select,
optgroup,
option {
    font-family: inherit;
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit
}

input,
button {
    overflow: visible;
    vertical-align: middle;
    outline: none
}

body,
th,
td,
button,
input,
select,
textarea {
    font-family: "Microsoft Yahei", "Hiragino Sans GB", "Helvetica Neue", Helvetica, tahoma, arial, Verdana, sans-serif, "WenQuanYi Micro Hei", "\5B8B\4F53";
    font-size: 14px;
    color: #333;
    -webkit-font-smoothing: antialiased;
    -moz-font-smoothing: antialiased
}

body {
    line-height: 1.6;
    background-color: #fff;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-size: 100%
}

a,
area {
    outline: none;
    blr: expression(this.onFocus=this.blur())
}

a {
    text-decoration: none;
    cursor: pointer;
    color: #333;
}

a:hover {
    text-decoration: none;
    outline: none
}

a.ie6:hover {
    zoom: 1
}

a:focus {
    outline: none
}

a:hover,
a:active {
    outline: none
}

:focus {
    outline: none
}


/*img*/

img {
    border: 0;
    vertical-align: middle
}

a img,
img {
    -ms-interpolation-mode: bicubic
}
/*clearfix*/

.cl:after,
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden
}

.cl,
.clearfix {
    zoom: 1
}

.wrap {
    width: 100%;
    margin: 0 auto;
}

/*activity*/
.activity{
	width: 100%;
}
.activity_top{
	width:100%;
	height:510px;
}
.activity_top .banner{
	width:100%;
	height:510px;
}
.activity_top .banner img{
	width:100%;
	height:100%;
}
.activity_content{
	float:left;
	width:100%;
	background:url(../images/activity_04_bg.png)center no-repeat;	
	background-size: 1920px,1722px;
}
.activity_content .content_center{
	width:1000px;
	height:1310px;
	margin:22px auto;
	background:url(../images/activity_04_center.png)center no-repeat;
}
.content_center ul{
	float:left;
	margin:144px 50px 106px 88px;
}
.content_center ul li {
	float:left;
	width:244px;
	height:326px;
	margin:15px;
}

.content_center ul li .item_img{
	width:174px;
	height:125px;
	float:left;
	margin:130px 18px 0 35px;
	-ms-transition:all 0.8s;
	-moz-transition:all 0.8s;
	-webkit-transition:all 0.8s;
	-o-transition:all 0.8s;
}
.content_center ul li:hover .item_img{
	-ms-transform: scale(1.2);
	-moz-transform: scale(1.2);
	-webkit-transform: scale(1.2);
	-o-transform: scale(1.2);
}
.content_center ul li .item_img img{
	width:100%;
	height:100%;
}
.content_center .item_1{
	background:url(../images/prize_01.jpg)center no-repeat;
}
.content_center .item_2{
	background:url(../images/prize_02.png)center no-repeat;
}
.content_center .item_3{
	background:url(../images/prize_03.jpg)center no-repeat;
}
.content_center .item_4{
	background:url(../images/prize_04.jpg)center no-repeat;
}
.content_center .item_5{
	background:url(../images/prize_05.jpg)center no-repeat;
}
.content_center .item_6{
	background:url(../images/prize_06.jpg)center no-repeat;
}
.content_center .item_7{
	background:url(../images/prize_07.png)center no-repeat;
}
.content_center .item_8{
	background:url(../images/prize_08.jpg)center no-repeat;
}
.activity_content .footer{
	width:1000px;
	margin:51px auto;
}
.activity_content .footer .title{
	margin:0 auto;
	width:894px;
	border-bottom: 1px solid #894e27;
	position:relative;
}
.activity_content .footer .title .title_i{
	background:#fff7dd;
	width:194px;
	height:30px;
	position:absolute;
	left:50%;
	top:-16px;
	margin-left:-110px;
	font:24px/30px 微软雅黑;
	color:#894e27;
	text-align: center;
}
.activity_content .footer .text{
	margin:51px auto;
	width:894px;
}
.activity_content .footer .text p{
	font:18px/35px 微软雅黑;
	color:#894e27;
	text-indent: -29px;
	padding-left: 29px;
	
}