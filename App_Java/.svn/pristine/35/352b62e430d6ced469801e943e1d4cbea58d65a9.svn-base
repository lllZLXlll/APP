*{margin:0;padding:0;border:0;}
	ul,li{list-style: none;}
	a{text-decoration: none;}
	body{font-family:'Microsoft Yahei';background-color:#fff;font-size:16px;line-height:1.5;color:#666666;}
	.main .top{width:100%;height:535px;background:url(../images/1101/1101_01.png) top center no-repeat;}
	.main .footer{width:100%;height:256px;background:url(../images/1101/1101_04.png) top center no-repeat;}
	.center{width:1200px;margin:-15px auto 0;}
	.center .title{padding-bottom:40px;background:url(../images/1101/1101_05.png) bottom center no-repeat}
	.center .title h3{height:55px;font-size:50px;color:#e6253a;line-height:55px;text-align:center;}
	.center .title p{height:45px;font-size:26px;color:#666666;text-align:center;}
	.content{width:100%;height:auto;overflow:hidden;margin-top:30px;}
	.content>div{float:left;height:500px;width:50%;}	
	.content .main-left{background:url(../images/1101/1101_02.png) top center no-repeat;}
	.content .main-right{background:url(../images/1101/1101_03.png) top center no-repeat;}
	.text{margin-top:20px;}
	.text p{font-size:22px;line-height:24px;text-align:center;margin-bottom:20px;}
	.text p span{color:#e6253a;}
	.text p.small{font-size:14px;line-height:24px;text-align:center;margin-bottom:0;}
	
	