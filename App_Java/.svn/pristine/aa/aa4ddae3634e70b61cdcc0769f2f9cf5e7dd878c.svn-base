@charset "utf-8";
*{margin:0;padding:0;border:0;}
.activity{
	width:100%;
	height:100%;
}

.img {
	width: 100%;
	height: auto;
}

.iosDiv, .androidDiv {
	width: 25%;
	height: auto;
	color: #fff;
	font-size: 0.14rem;
	text-align: center;
	line-height: 0.45rem;
}

.text {
	position:absolute;
	top: 0;
	left: 0.6rem;
	color: #fff;
}

.iosDiv {
	position:absolute;
	top: 2.3rem;
	left: 0.4rem;
}

.androidDiv {
	position:absolute;
	top: 2.9rem;
	left: 0.4rem;
}

