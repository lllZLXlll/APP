.explian a p.img{
	-webkit-transition: all 0.8s;
	transition: all 0.8s;
	-webkit-transition-delay:0.1s;
	transition-delay:0.1s;
	-ms-transform:translateY(0);
	-webkit-transform:translateY(0);
	transform:translateY(0);
	}
