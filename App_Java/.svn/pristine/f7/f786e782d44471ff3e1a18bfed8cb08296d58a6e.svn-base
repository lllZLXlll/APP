/*pull*/
.item .pull-box{float:left;margin-left:12px;}
.pull-box{position:relative;width:290px;height:40px;background-color:#e4e4e4;}
.pull-bg{width:0;height:40px;background-color:#72c713;position:absolute;top:0;left:0;z-index:2;}
.pull-default{
	width:100%;height:40px;
	line-height:40px;
	text-align:center;
	position:absolute;top:0;left:0;z-index:3;
	-webkit-background-clip: text;    /*按文字裁剪*/        
    -webkit-text-fill-color: transparent;    /*文字的颜色使用背景色*/  
    background-color:#e4e4e4;    /*设置一个背景色*/        
    background-image: -webkit-linear-gradient(-45deg, rgba(0, 0, 0, 0.6) 30%, #aff0ff 50%, rgba(0, 0, 0, 0.6) 70%);        /*设置渐变的背景，按对角线渐变*/
    background-blend-mode: hard-light;    /*设置背景为混合模式下的强光模式*/
    background-size: 200%;
    -webkit-animation: shine 4s infinite;    /*给背景添加动画改变位置*/
}
@-moz-document url-prefix() { .pull-default { background-color:transparent!important; } }

.pull-default-true{width:100%;height:40px;color:#fff;
	line-height:40px;
	text-align:center;
	position:absolute;top:0;left:0;z-index:3;}
@keyframes shine {
    from {background-position: 100%;}
    to {background-position: 0;}
}
@-webkit-keyframes shine {
    from {background-position: 100%;}
    to {background-position: 0;}
}
.pull-btn{
	width:38px;
	height:38px;
	cursor: move;
	border:1px solid #acacac;
	background:url(../shImages/right.png) no-repeat center center;
	line-height:40px;text-align:center;background-color:#fff;position:absolute;left:0;top:0;z-index:4;
}
.pull-btn-true{background:url(../shImages/pull-true.png) no-repeat center center;background-color:#fff;}



